{
  "name": "spheremall/ms-client",
  "description": "SphereMall API client for SphereMall GateWay",
  "type": "library",
  "config": {
    "vendor-dir": "vendor"
  },
  "autoload": {
    "psr-4": {
      "SphereMall\\MS\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SphereMall\\MS\\Tests\\": "tests/"
    }
  },
  "require"     : {
    "php"                        : "^7.0",
    "guzzlehttp/guzzle"          : "~6.0",
    "elasticsearch/elasticsearch": "~6.0"
  },
  "authors": [
    {
      "name": "Sergey Kondratovec",
      "email": "sergey@spheremall.com"
    }
  ],
  "version": "2.7.19",
  "require-dev": {
    "phpunit/phpunit": "6.2"
  }
}
